{
    "name": "decodelabs/disciple",
    "description": "Take control of your users",
    "type": "library",
    "keywords": [ "user", "client" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/compass": "^0.3",
        "decodelabs/exceptional": "^0.6",
        "decodelabs/monarch": "^0.1.3",
        "decodelabs/veneer": "^0.12.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Disciple\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
